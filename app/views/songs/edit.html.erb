<h1>Editing song</h1>

<% form_for(@song) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :artist %><br />
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :album %><br />
    <%= f.text_field :album %>
  </p>
  <p>
    <%= f.label :filename %><br />
    <%= f.text_field :filename %>
  </p>
  <p>
    <%= f.label :art_filename %><br />
    <%= f.text_field :art_filename %>
  </p>
  <p>
    <%= f.label :submitter_name %><br />
    <%= f.text_field :submitter_name %>
  </p>
  <p>
    <%= f.label :playlist_idx %><br />
    <%= f.text_field :playlist_idx %>
  </p>
  <p>
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @song %> |
<%= link_to 'Back', songs_path %>